               <VBox.margin>
                  <Insets bottom="50.0" />
               </VBox.margin>
            </Label>
            <VBox minHeight="200.0" prefHeight="460.0" prefWidth="1000.0">
               <children>
                  <HBox prefHeight="349.0" prefWidth="1000.0">
