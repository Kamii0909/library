<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox fx:id="hBoxContent" maxWidth="2000.0" prefHeight="500.0" prefWidth="1000.0"
   xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
   fx:controller="hust.kien.project.controller.PhieuMuonController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="0.0" prefWidth="2000.0">
         <children>
            <Label text="Danh sách phiếu mượn">
               <font>
                  <Font name="System Bold" size="22.0" />
               </font>
            </Label>
            <TextField fx:id="txt_search" onKeyReleased="#timKiemSach" prefHeight="32.0"
               prefWidth="2000.0" promptText="tìm kiếm phiếu mượn" />
            <TableView fx:id="table" maxWidth="2000.0" prefHeight="2000.0" prefWidth="2000.0">
               <columns>
                  <TableColumn fx:id="col_id" prefWidth="75.0" text="id" />
                  <TableColumn fx:id="col_ngaybatdau" prefWidth="117.0" text="ngày bắt đầu" />
                  <TableColumn fx:id="col_ngayketthuc" prefWidth="146.0" text="ngày kết thúc" />
                  <TableColumn fx:id="col_sach" prefWidth="134.0" text="sách" />
                  <TableColumn fx:id="col_nguoidung" prefWidth="134.0" text="người dùng" />
               </columns>
            </TableView>
            <VBox minHeight="200.0" prefHeight="200.0" prefWidth="2000.0">
               <children>
                  <Label text="Thêm/ chỉnh sửa thông tin mượn sách">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                     </VBox.margin>
                  </Label>
                  <HBox prefHeight="184.0" prefWidth="2000.0">
                     <children>
                        <VBox alignment="TOP_CENTER" prefHeight="159.0" prefWidth="140.0">
                           <children>
                              <Label text="id">
                                 <font>
                                    <Font size="15.0" />
                                 </font>
                              </Label>
                              <Label text="ngày bắt đầu">
                                 <font>
                                    <Font size="15.0" />
                                 </font>
                                 <VBox.margin>
                                    <Insets top="27.0" />
                                 </VBox.margin>
                              </Label>
                              <Label text="ngày kết thúc">
                                 <font>
                                    <Font size="15.0" />
                                 </font>
                                 <VBox.margin>
                                    <Insets top="23.0" />
                                 </VBox.margin>
                              </Label>
                           </children>
                        </VBox>
                        <VBox prefHeight="159.0" prefWidth="201.0">
                           <children>
                              <TextField fx:id="txt_id" />
                              <DatePicker fx:id="date_ngaybatdau" prefHeight="25.0"
                                 prefWidth="245.0">
                                 <VBox.margin>
                                    <Insets top="20.0" />
                                 </VBox.margin>
                              </DatePicker>
                              <DatePicker fx:id="date_ngayketthuc" prefHeight="25.0"
                                 prefWidth="246.0">
                                 <VBox.margin>
                                    <Insets top="20.0" />
                                 </VBox.margin>
                              </DatePicker>
                           </children>
                        </VBox>
                        <VBox alignment="TOP_CENTER" prefHeight="159.0" prefWidth="140.0">
                           <HBox.margin>
                              <Insets left="20.0" />
                           </HBox.margin>
                           <children>
                              <Label text="sách">
                                 <font>
                                    <Font size="15.0" />
                                 </font>
                              </Label>
                              <Label text="người dùng">
                                 <font>
                                    <Font size="15.0" />
                                 </font>
                                 <VBox.margin>
                                    <Insets top="27.0" />
                                 </VBox.margin>
                              </Label>
                           </children>
                        </VBox>
                        <VBox prefHeight="159.0" prefWidth="201.0">
                           <children>
                              <ComboBox fx:id="choice_sach" prefHeight="25.0" prefWidth="213.0" />
                              <ComboBox fx:id="choice_nguoidung" prefHeight="25.0" prefWidth="213.0">
                                 <VBox.margin>
                                    <Insets top="20.0" />
                                 </VBox.margin>
                              </ComboBox>
                           </children>
                        </VBox>
                        <VBox alignment="TOP_CENTER" prefHeight="205.0" prefWidth="253.0">
                           <children>
                              <Button mnemonicParsing="false" onAction="#themKhuyenMai"
                                 prefHeight="35.0" prefWidth="283.0"
                                 style="-fx-background-color: #3f4a59;" text="Thêm phiếu mượn"
                                 textFill="WHITE">
                                 <opaqueInsets>
                                    <Insets />
                                 </opaqueInsets>
                                 <font>
                                    <Font name="System Bold" size="16.0" />
                                 </font>
                              </Button>
                              <Button mnemonicParsing="false" onAction="#suaKhuyenMai"
                                 prefHeight="35.0" prefWidth="306.0"
                                 style="-fx-background-color: #3f4a59;" text="Sửa phiếu mượn"
                                 textFill="WHITE">
                                 <opaqueInsets>
                                    <Insets />
                                 </opaqueInsets>
                                 <VBox.margin>
                                    <Insets top="20.0" />
                                 </VBox.margin>
                                 <font>
                                    <Font name="System Bold" size="16.0" />
                                 </font>
                              </Button>
                              <Button mnemonicParsing="false" onAction="#xoaKhuyenMai"
                                 prefHeight="35.0" prefWidth="313.0"
                                 style="-fx-background-color: #3f4a59;" text="Xóa phiếu mượn"
                                 textFill="WHITE">
                                 <opaqueInsets>
                                    <Insets />
                                 </opaqueInsets>
                                 <VBox.margin>
                                    <Insets top="20.0" />
                                 </VBox.margin>
                                 <font>
                                    <Font name="System Bold" size="16.0" />
                                 </font>
                              </Button>
                           </children>
                           <HBox.margin>
                              <Insets left="20.0" />
                           </HBox.margin>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets top="15.0" />
   </padding>
</HBox>